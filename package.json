{
  "name": "skeleton-parser",
  "description": "parses a skeleton project structure",
  "main": "index.js",
  "bin": "cli.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "lint": "xo",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "test:tdd": "jest --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucalanca/skeleton-parser.git"
  },
  "keywords": [
    "cli-app",
    "cli",
    "skeleton",
    "parser"
  ],
  "author": "lucalanca",
  "license": "Apache-2.0",
  "dependencies": {
    "audio-extensions": "0.0.0",
    "font-extensions": "^1.0.0",
    "fs-promise": "^0.5.0",
    "globby": "^5.0.0",
    "gonzales-pe": "^4.0.3",
    "image-extensions": "^1.1.0",
    "js-yaml": "^3.6.1",
    "lodash.isarray": "^4.0.0",
    "lodash.merge": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "meow": "^3.7.0",
    "postcss": "^5.2.15",
    "postcss-scss": "^0.4.1",
    "pug": "^2.0.0-beta11",
    "pug-lexer": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "chalk": "^1.1.3",
    "difflet": "^1.0.1",
    "jest": "^19.0.2",
    "semantic-release": "^4.3.5",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "ava"
    ]
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2"
      ],
      "plugins": [
        "transform-runtime"
      ],
      "comments": false
    }
  },
  "git-pre-hooks": {
    "pre-push": "npm test"
  }
}
